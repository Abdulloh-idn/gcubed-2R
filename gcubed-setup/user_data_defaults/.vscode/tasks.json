{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Open graphing page",
            "command": "python -m gcubed.graphics.hypercube",
            "type": "shell",
            "presentation": {
                "reveal": "silent"
            },
            "isBackground": true,
            "group": "none",
            "problemMatcher": []
        },
        {
            "label": "Auto-open error message file.",
            "type": "shell",
            "command": "code error-message.md",
            "runOptions": {
                "runOn": "folderOpen",
            },
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": false,
            }
        }
    ]
}
